{
  "name": "sfdx-1commerce-plugin",
  "description": "1Commerce SFDX Commands",
  "version": "0.0.5",
  "author": "Greg Smolyn",
  "bugs": "https://github.com/forcedotcom/sfdx-1commerce-plugin/issues",
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^3.0.3",
    "@salesforce/core": "^2.12.3",
    "@salesforce/ts-types": "^1.4.3",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "tslint-to-eslint-config": "^1.5.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^3.2.0",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "2.0.0",
    "@salesforce/ts-sinon": "^1.2.3",
    "@types/chai": "^4.2.13",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.3",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "chai": "^4",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.6.3",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-tslint": "^3.1.0",
    "globby": "^11.0.1",
    "mocha": "^8.1.3",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "rimraf": "^3.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forcedotcom/sfdx-1commerce-plugin",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "1commerce:search": {
        "description": "commands for search"
      },
      "1commerce:import": {
        "description": "commerce data import tools"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "forcedotcom/sfdx-1commerce-plugin",
  "scripts": {
    "build": "rimraf lib && tsc",
    "clean": "rimraf oclif.manifest.json; rimraf yarn-error.log; rimraf package-lock.json",
    "prepare": "yarn run build && oclif-dev manifest",
    "preversion": "yarn run clean",
    "test": "nyc --extension .ts mocha \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "prettier": "prettier --write \"**/*.(ts|json)\""
  },
  "eslintConfig": {
    "extends": [
      "oclif",
      "oclif-typescript",
      "airbnb-base",
      "airbnb-typescript/base",
      "prettier/@typescript-eslint",
      "prettier"
    ],
    "parserOptions": {
      "project": "tsconfig.json",
      "ecmaVersion": 2019
    },
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-await-in-loop": "warn",
      "no-plusplus": "warn",
      "no-use-before-define": "off",
      "import/prefer-default-export": "warn",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ]
    }
  }
}
